
   {
     "compilerOptions": {
       "target": "ES2022",
       "module": "ESNext",
       "strict": false,
       "esModuleInterop": true,
       "skipLibCheck": true,
       "forceConsistentCasingInFileNames": false,
       "outDir": "./dist",
       "rootDir": "./",
       "moduleResolution": "node",
       "resolveJsonModule": true
     },
     "include": ["/**/*"],
     "exclude": ["node_modules"]
   }
   
